pipeline {
    agent any

    stages {
        stage('Pipeline 1') {
            steps {
                echo 'Hello World'
            }
            
        }
        stage('Pipeline 2') {
            steps {
                echo 'Good-bye World'
            }
        }
        stage('Pipeline 3') {
            steps {
                echo 'toto'
            }
        }
    }
}
